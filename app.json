{
  "expo": {
    "name": "Î“Î¹Î± ÎµÎ¼Î¬Ï‚ ðŸ’–",
    "slug": "our_journal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ourjournal",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.johnkar.ourjournal",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
